{
  "name": "atom-tap-test-runner",
  "version": "3.0.0",
  "description": "Run Atom package tests using TAP",
  "license": "MIT",
  "keywords": [
    "atom",
    "test",
    "anything",
    "tap"
  ],
  "repository": "https://github.com/wooorm/atom-tap-test-runner",
  "bugs": "https://github.com/wooorm/atom-tap-test-runner/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "debug": "^2.2.0",
    "glob": "^7.0.5",
    "tap-parser": "^5.0.1",
    "tmp": "^0.0.30",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "execa": "^0.6.0",
    "remark-cli": "^3.0.0",
    "remark-preset-wooorm": "^2.0.0",
    "tape": "^4.6.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build": "remark . -qfo",
    "lint": "xo",
    "test-api": "tape test",
    "test": "npm run build && npm run lint && npm run test-api"
  },
  "atomTestRunner": "./index.js",
  "xo": {
    "space": true
  },
  "remarkConfig": {
    "plugins": ["preset-wooorm"]
  }
}
